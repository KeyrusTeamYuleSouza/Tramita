
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title - Tramita 2.0</title>
    <!--  Use Bundles And Modernizr Here -->
    <!-- Global Mandatory Style -->
    <link href="~/Content/theme/metronic/assets/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/theme/metronic/assets/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/theme/metronic/assets/global/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/theme/metronic/assets/global/plugins/uniform/css/uniform.default.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/theme/metronic/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css" rel="stylesheet" type="text/css" />
    <!--  Global Mandatory Style -->
    <!-- Page Level Plugin Styles -->
    <link href="~/Content/theme/metronic/assets/global/plugins/gritter/css/jquery.gritter.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/theme/metronic/assets/global/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css" rel="stylesheet" type="text/css" />

    <link href="~/Content/theme/metronic/assets/global/plugins/fullcalendar/fullcalendar.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/theme/metronic/assets/global/plugins/jqvmap/jqvmap/jqvmap.css" rel="stylesheet" type="text/css" />
    <!-- Page Level Plugin Styles -->
    <!--  Page Styles -->
    <link href="~/Content/theme/metronic/assets/admin/pages/css/tasks.css" rel="stylesheet" type="text/css" />
    <!-- Page Styles -->
    <!-- Theme Styles -->
    <link href="~/Content/theme/metronic/assets/global/css/components.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/theme/metronic/assets/global/css/plugins.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/theme/metronic/assets/admin/layout/css/layout.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/theme/metronic/assets/admin/layout/css/themes/default.css" rel="stylesheet" type="text/css" id="style_color" />
    <link href="~/Content/theme/metronic/assets/admin/layout/css/custom.css" rel="stylesheet" type="text/css" />
    <!-- Theme Styles -->

</head>
<body>
    <!-- Header -->
    @Html.Partial("_HeaderPartial")
    <!-- Header -->

    <!-- Sidebar and Quick Sidebar -->
    @Html.Partial("_SideBarPartial")
    <!-- Sidebar and Quick Sidebar -->

    <!-- Content -->
    <div class="page-content-wrapper">
        <div class="page-content">
            @RenderBody()
        </div>
    </div>
    <!-- Content -->

    <!-- Footer -->
    <div class="page-footer">
        <div class="page-footer-inner">
            2017 &copy; Tramita 2.0 by keenthemes.
        </div>
        <div class="page-footer-tools">
            <span class="go-top">
                <i class="fa fa-angle-up"></i>
            </span>
        </div>
    </div>

    <script src="~/Content/theme/metronic/assets/global/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/jquery-migrate.min.js" type="text/javascript"></script>
    <!-- IMPORTANT! Load jquery-ui-1.10.3.custom.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->

    <script src="~/Content/theme/metronic/assets/global/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>

    <script src="~/Content/theme/metronic/assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/jquery.cokie.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/uniform/jquery.uniform.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="~/Content/theme/metronic/assets/global/plugins/flot/jquery.flot.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/flot/jquery.flot.resize.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/flot/jquery.flot.categories.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/jquery.pulsate.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/bootstrap-daterangepicker/daterangepicker.js" type="text/javascript"></script>
    <!-- IMPORTANT! fullcalendar depends on jquery-ui-1.10.3.custom.min.js for drag & drop support -->

    <script src="~/Content/theme/metronic/assets/global/plugins/jquery-easypiechart/jquery.easypiechart.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/jquery.sparkline.min.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/global/plugins/gritter/js/jquery.gritter.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="~/Content/theme/metronic/assets/global/scripts/metronic.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/admin/layout/scripts/layout.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/admin/layout/scripts/quick-sidebar.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/admin/pages/scripts/index.js" type="text/javascript"></script>
    <script src="~/Content/theme/metronic/assets/admin/pages/scripts/tasks.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL SCRIPTS -->

    @RenderSection("scripts", required: false)

    <script>
        jQuery(document).ready(function () {
            Metronic.init();
            Layout.init();
            QuickSidebar.init()
            Index.init();
            Index.initCharts();
            Index.initChat();
            Index.initMiniCharts();
            Tasks.initDashboardWidget();
        });
    </script>
</body>
</html>

